FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      P_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField  uniform 1e5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }
    Walls
    {
        type            zeroGradient;
    }
    Air_to_battery1
    {
        type           zeroGradient;
    }
}


